{
    "name": "codefog/contao-instagram",
    "description": "Instagram bundle for Contao Open Source CMS",
    "keywords": [
        "contao",
        "instagram",
        "feed"
    ],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        },
        {
            "name": "Kamil Kuzminski",
            "homepage": "https://github.com/qzminski",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-json": "*",
        "contao/core-bundle": "^4.9 || ^5.0",
        "guzzlehttp/guzzle": "~6.0",
        "kevinrob/guzzle-cache-middleware": "^3.2",
        "doctrine/cache": "^1.9",
        "doctrine/dbal": "^2.10 || ^3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\InstagramBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\InstagramBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "ocramius/package-versions": true,
            "contao-community-alliance/composer-plugin": true,
            "contao/manager-plugin": true
        }
    }
}
